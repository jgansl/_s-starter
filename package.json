  "keywords": [
    "WordPress",
    "Theme"
  ],
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://@github.com/"
  },
  "bugs": {
    "url": "https://github.com/issues"
  },
  "dependencies": {
    "bootstrap-scss": "^5.2.2",
    "gsap": "^3.10.4",
    "loadjs": "^4.2.0",
    "lozad": "^1.16.0"
  },
  "devDependencies": {
    "@wordpress/scripts": "^19.2.2",
    "dir-archiver": "^1.1.1",
    "@prettier/plugin-php": "^0.18.9",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "dotenv": "^16.0.3",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rollup": "^2.39.1",
    "rollup-plugin-browsersync": "^1.1.0",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^5.3.1",
    "rtlcss": "^3.5.0",
    "rxjs": "^7.5.6",
    "sass": "^1.26.9",
    "svelte": "^3.52.0",
    "svelte-preprocess": "^4.10.7",
    "typescript": "^4.7.4",
    "vite": "^3.2.0",
    "vite-plugin-browser-sync": "^1.0.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "build": "run-p rollup-build sass-build",
    "dev": "rollup -c -w",
    "lh": "lhci autorun",
    "package": "run-s build zip",
    "xpostinstall": "chmod +x scripts/ssl.sh && scripts/ssl.sh && npm run build && if [ ! -f \".env.local\" ]; then cp .example.env.local .env.local; fi;",
    "postinstall": "chmod +x scripts/ssl.sh && scripts/ssl.sh && npm run build",
    "rollup-build": "rollup -c",
    "sass-build": "sass --style compressed styles/theme.scss:public/css/bundle.css",
    "start": "run-p dev sass-watch",
    "watch": "sass --watch styles/theme.scss:public/css/bundle.css styles/editor.scss:public/css/editor.css",
    "zip": "mkdir -p packaged && zip -rq packaged/theme_$(date '+%Y-%m-%d-%H-%M').zip ./* -x *.git* -x node_modules/\\* -x scripts/\\* -x debug.txt -x packaged/\\* && open packaged",
     
    "s:watch": "node-sass styles/ -o ./ --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass styles/ -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'styles/**/*.scss'",
    "lint:js": "wp-scripts lint-js 'js/*.js'",
    "bundle": "dir-archiver --src . --dest ../_s.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map yarn.lock"
  }
}
